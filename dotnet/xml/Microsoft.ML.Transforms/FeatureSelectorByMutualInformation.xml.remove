    <example name="MutualInformationFeatureSelection">
      <example>
        <code language="csharp">
          pipeline.Add(new FeatureSelectorByMutualInformation
          { 
            Column = new[]{ "Feature1" }, 
            SlotsInOutput = 6 
           });
